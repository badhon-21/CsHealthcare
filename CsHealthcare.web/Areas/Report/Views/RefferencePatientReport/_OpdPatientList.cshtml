@model IEnumerable<CsHealthcare.web.Areas.Report.Controllers.RefferencePatientReportController.RefferenceSummaryModel>


<table class="table table-striped table-bordered table-hover" id="listOpdPatientList">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PatientCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PatientName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MerketingBy)
            </th>



        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Date.ToString("d")
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PatientCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PatientName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MerketingBy)
                </td>


            </tr>
        }
    </tbody>
</table>



