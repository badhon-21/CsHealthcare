@model IEnumerable<CsHealthcare.ViewModel.MedicineCorner.DrugSearchModel>

<div class="col-md-12 text-center">
    <h3>Buffer Stock Report</h3>

</div>
<table class="table">
    <tr>
        <th>
            Stock Date
        </th>
        <th>
            Drug Name
        </th>
        <th>
            Stock Quantity
        </th>


    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @item.BufferStockDate.ToString("d")
            </td>
            <td>
                @item.TradeName
            </td>
            <td>
                @item.DepartmentwiseStockInQuantity
            </td>


        </tr>
    }

</table>


