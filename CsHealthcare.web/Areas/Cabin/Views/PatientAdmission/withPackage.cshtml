    <script type="text/javascript">
                                    jQuery(document).ready(function() {
                                        allowInt();
                                        //initPatientTestAutoComplete();
                                        initCabinAutoComplete();
                                        //initPurposeAutoComplete();


                                        LoadPackage();
                                        ComponentsPickers.init();
                                        $('#txtDateOfBirth').val(getTodate());

                                        $(".datetimepicker").datepicker();

                                        $('#CabinRentDate').on('change', function() {
                                            SelectDate();

                                        });

                                        $('#PackageId').on('change', function() {
                                            PackageAmount();

                                        });
                                    });


                                    function getTodate() {
                                        var dob = new Date();
                                        var day = (dob.getDate() < 10 ? '0' : '') + dob.getDate();
                                        var month = ((dob.getMonth() + 1) < 10 ? '0' : '') + (dob.getMonth() + 1);
                                        return (month + "/" + day + "/" + dob.getFullYear());
                                    }

                                    function allowInt() {
                                        $(".integer").format({ precision: 0, allow_negative: false, autofix: true });
                                    }


                                    function Search() {
                                        $.ajax({
                                            type: "GET",
                                            cache: false,
                                            url: "@(@Url.Action("SearchPatient", "PatientAdmission", new {Area = "Cabin"}))",
                                            data: { "PatientCode": $("#PatientCode").val() },
                                            dataType: "html",
                                            success: function(result) {

                                                var t = JSON.parse(result);
                                                //alert(t);

                                                $('#PatientName').val(t.PatientName);
                                                $('#PatientId').val(t.PatientId);
                                                //$('#Total').val(t.T_Price);
                                                // $('#quantity').focus();
                                            },
                                            error: function(error) {
                                                //Metronic.unblockUI();
                                                var msg = 'Fail to load .';
                                                alert(msg);
                                            }
                                        });
                                    }


                                    function initPatientTestAutoComplete() {
                                        $('#PatientName').typeahead({
                                            hint: true,
                                            highlight: true,
                                            minLength: 2,
                                            limit: 8
                                        }, {
                                            source: function(searchString, cb) {
                                                return $.ajax({
                                                    dataType: 'json',
                                                    type: 'POST',
                                                    contentType: "application/json; charset=utf-8",
                                                    url: " /Cabin/PatientAdmission/LoadPatient",
                                                    data: "{'SearchString':'" + searchString + "'}",
                                                    chache: false,
                                                    success: function(data) {
                                                        var result = [];
                                                        $.each(data, function(index, val) {
                                                            result.push({
                                                                Id: val.Id,
                                                                Name: val.Name,
                                                                value: val.Name
                                                            });
                                                        });
                                                        cb(result);
                                                    }
                                                });
                                            }
                                        }).on("typeahead:selected", function(obj, patient) {
                                            $("#tId").val(patient.Id);
                                            $("#PatientId").val(patient.Id);
                                            //$("#PatientCode").val(patient.PatientCode);
                                            LoadPatientCode();
                                            LoadPatientName();
                                            //$("#txtPatientName").val(patient.Name);
                                            //$("#txtMobileNumber").val(patient.Mobile);
                                        });

                                    }

                                    function LoadPatientCode() {
                                        $.ajax({
                                            type: "GET",
                                            cache: false,
                                            url: "@(@Url.Action("PatientCode", "PatientAdmission", new {Area = "Cabin"}))",
                                            data: { "Id": $("#tId").val() },
                                            dataType: "html",
                                            success: function(result) {

                                                var t = JSON.parse(result);

                                                if (t == null) {

                                                    $('#New').show();

                                                } else {

                                                    $('#PatientCode').val(t);
                                                    $('#New').hide();
                                                }


                                                //$('#Total').val(t.T_Price);
                                                // $('#quantity').focus();
                                            },
                                            error: function(error) {
                                                //Metronic.unblockUI();
                                                var msg = 'Fail to load .';
                                                alert(msg);
                                            }
                                        });
                                    }

                                    function LoadPatientName() {
                                        $.ajax({
                                            type: "GET",
                                            cache: false,
                                            url: "@(@Url.Action("PatientName", "PatientAdmission", new {Area = "Cabin"}))",
                                            data: { "Id": $("#tId").val() },
                                            dataType: "html",
                                            success: function(result) {

                                                var t = JSON.parse(result);
                                                //alert(t);

                                                $('#PatientName').val(t);
                                                //$('#Total').val(t.T_Price);
                                                // $('#quantity').focus();
                                            },
                                            error: function(error) {
                                                //Metronic.unblockUI();
                                                var msg = 'Fail to load .';
                                                alert(msg);
                                            }
                                        });
                                    }

                                    function initCabinAutoComplete() {
                                        $('#CabinName').typeahead({
                                            hint: true,
                                            highlight: true,
                                            minLength: 2,
                                            limit: 8
                                        }, {
                                            source: function(searchString, cb) {
                                                return $.ajax({
                                                    dataType: 'json',
                                                    type: 'POST',
                                                    contentType: "application/json; charset=utf-8",
                                                    //url: " /Cabin/PatientAdmission/LoadCabin",
                                                     url: "@(@Url.Action("LoadCabin", "Appointment", new {Area = "AppointmentSystem"}))",
                        data: "{'SearchString':'" + searchString + "'}",
                                                    chache: false,
                                                    success: function(data) {
                                                        var result = [];
                                                        $.each(data, function(index, val) {
                                                            result.push({
                                                                Id: val.Id,
                                                                Name: val.Name,
                                                                value: val.Name
                                                            });
                                                        });
                                                        cb(result);
                                                    }
                                                });
                                            }
                                        }).on("typeahead:selected", function(obj, patient) {
                                            $("#cId").val(patient.Id);
                                            $("#CabinId").val(patient.Id);
                                            CabinAmount();
                                            //getTotal();
                                            //$("#txtPatientName").val(patient.Name);
                                            //$("#txtMobileNumber").val(patient.Mobile);
                                        });

                                    }

                                    function LoadPackage() {
                                        //Metronic.blockUI();
                                        $('#PackageId').find('option').remove();
                                        var options = $("#PackageId");
                                        $.ajax({
                                            type: "GET",
                                            cache: false,
                                            url: "@(@Url.Action("LoadPackage", "PatientAdmission", new {Area = "Cabin"}))",
                                            data: {},
                                            dataType: "html",
                                            success: function(result) {
                                                //Metronic.unblockUI();
                                                var t = JSON.parse(result);
                                                options.append($("<option />").val("").text("-Select -"));
                                                $.each(t, function() {
                                                    options.append($("<option />").val(this.Id).text(this.Name));
                                                });
                                            },
                                            error: function(xhr, ajaxOptions, thrownError) {
                                                //Metronic.unblockUI();
                                                var msg = 'Fail to load  list.';
                                                alert(msg);
                                            }
                                        });
                                    }


                                    function CabinAmount() {
                                        $.ajax({
                                            type: "GET",
                                            cache: false,
                                            url: "@(@Url.Action("LoadPrice", "PatientAdmission", new {Area = "Cabin"}))",
                                            data: { "Id": $("#cId").val() },
                                            dataType: "html",
                                            success: function(result) {

                                                var t = JSON.parse(result);

                                                $('#Amount').val(t);
                                                //$('#Total').val(t.T_Price);
                                                // $('#quantity').focus();
                                            },
                                            error: function(error) {
                                                //Metronic.unblockUI();
                                                var msg = 'Fail to load .';
                                                alert(msg);
                                            }
                                        });
                                    }


                                    function getTotal() {
                                        $.ajax({
                                            type: "GET",
                                            cache: false,
                                            url: "@(@Url.Action("GetTotal", "PatientAdmission", new {Area = "Cabin"}))",
                                            data: { "Amount": $("#Amount").val(), "AdmissionFee": $("#AdmissionFee").val() },
                                            dataType: "html",
                                            success: function(result) {

                                                //var t = JSON.parse(result);

                                                $('#sub_total').val(result);
                                                $('#TotalAmount').val(result);
                                                //$('#Total').val(t.T_Price);
                                                // $('#quantity').focus();
                                            },
                                            error: function(error) {
                                                //Metronic.unblockUI();
                                                var msg = 'Fail to load .';
                                                alert(msg);
                                            }
                                        });
                                    }

                                    function calculate() {
                                        var myBox1 = document.getElementById('sub_total').value;
                                        var myBox2 = document.getElementById('DeuAmount').value;
                                        //var result = document.getElementById('SubTotal');
                                        var total = document.getElementById('GrandTotal');
