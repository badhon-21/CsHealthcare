@model IEnumerable<CsHealthcare.ViewModel.Patient.PatientSpecialNoteModel>


<table class="table table-striped table-bordered table-hover" id="tabSpecialNote">
    <thead>
        <tr>
            <th style="width: 90% !important">
                @Html.DisplayNameFor(model => model.SpecialNote)
            </th>
            <th style="width: 10% !important">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="width: 90% !important; font-family: SutonnyMJ;">
                    @Html.DisplayFor(modelItem => item.SpecialNote)
                </td>
                <td style="width: 10% !important">
                    <a class="btn btn-xs green" title="Delete" onclick="javascript: if (confirm('Are you sure you want to do operation?') == false) return false; DeletePatientSpecialNote(@item.PatientPrescriptionId, @item.SpecialNoteId);"><i class="fa fa-trash-o"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>




