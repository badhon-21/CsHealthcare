@model IEnumerable<CsHealthcare.ViewModel.Patient.PatientPrescriptionModel>


<table class="table table-striped table-bordered table-hover" id="tabWaitingList">
    <thead>
        <tr>
            <th style="width: 15% !important">
                Date
            </th>
            <th style="width: 10% !important">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="width: 15% !important">
                    @item.Date.ToString("d")
                </td>

                <td style="width: 10% !important">
                    <!--a class="btn btn-xs green" Target="_blank" title="Previous Prescription" href="@(Url.Action("SetPrescription", "Prescription", new { PatientId = item.PatientId, DoctorId=item.DoctorId, appDate=item.Date }))" data-modal=""><i class="fa fa-heartbeat"></i></a-->
                    <a class="btn btn-xs green" Target="_blank" title="Previous Prescription" href="@(Url.Action("ViewPrescription", "Prescription", new { PatientId = item.PatientId}))" data-modal=""><i class="fa fa-heartbeat"></i></a>

                </td>
            </tr>
        }
    </tbody>
</table>




