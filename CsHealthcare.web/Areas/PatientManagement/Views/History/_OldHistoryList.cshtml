@model IEnumerable<CsHealthcare.ViewModel.Patient.PatientHistoryMiniModel>


<table class="table table-striped table-bordered table-hover" id="tabWaitingList">
    <thead>
        <tr>
            <th style="width: 15% !important">
                Date
            </th>
            <th style="width: 10% !important">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="width: 15% !important">
                    @item.HistoryTakenDate.Date.ToString("d")
                </td>

                <td style="width: 10% !important">
                    <a class="btn btn-xs green" Target="_blank" title="Previous History" href="@(Url.Action("LoadHistory", "History", new { HistoryId = item.Id}))"><i class="glyphicon glyphicon-pencil"></i></a>

                </td>
            </tr>
        }
    </tbody>
</table>




