@model IEnumerable<CsHealthcare.ViewModel.Hospital.BedModel>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.BedType)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Price)

        </td>

        <td style="width: 15% !important">
            <a class="btn btn-primary btn-icon legitRipple" title="Edit" onclick="javascript: EditBed('@item.Name');"><i class="icon-pencil"></i></a>
            @*<a class="btn btn-xs btn-danger" title="Delete" onclick="javascript: if (confirm('Are you sure you want to do operation?') == false) return false; DeleteTestDetails('@item.Id','@item.TestNameId'); "><i class="icon-trash"></i></a>*@
        </td>

     
        
    </tr>
}


