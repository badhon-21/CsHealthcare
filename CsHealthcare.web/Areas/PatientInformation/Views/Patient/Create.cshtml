        function getTodate() {
            var dob = new Date();
            var day = (dob.getDate() < 10 ? '0' : '') + dob.getDate();
            var month = ((dob.getMonth() + 1) < 10 ? '0' : '') + (dob.getMonth() + 1);
            return (month + "/" + day + "/" + dob.getFullYear());
        }

        function allowInt() {
            $(".integer").format({ precision: 0, allow_negative: false, autofix: true });
        }


        function initPatientType() {

            $.ajax({
                type: "GET",
                cache: false,
                url: "@(@Url.Action("loadPatientType", "Patient", new {Area = "PatientInformation" }))",
                data: { "PatientType": $('#radOld').val() },
                dataType: "html",
                success: function (result) {
                    //var value = JSON.parse(result);

                    if (result != "New") {
                        $('#txtId').attr("disabled", false);
