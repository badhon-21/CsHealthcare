        function calculate() {

            var price = document.getElementById('Price').value;
            var qty = document.getElementById('Quantity').value;

           // var myBox1 = document.getElementById('Total').value;
           var sDiscount = document.getElementById('TestDiscount').value;
            //var result = document.getElementById('SubTotal');
            var total = document.getElementById('Total');
            //var total1 = document.getElementById('DeuPrice');
            var sPrice = price * qty;
            var myResult = sPrice - (sPrice * (sDiscount / 100)).toFixed(2);;
            total.value = myResult;
            //total.value = total;
        }
