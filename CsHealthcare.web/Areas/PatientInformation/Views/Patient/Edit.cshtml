            // }
        }



        function EditTest(TestNameId) {
            $.ajax({
                type: "GET",
                cache: false,
                url: "@(@Url.Action("EditTestDetails", "Patient", new {Area = "PatientInformation" }))",
                data: { "TestId": TestNameId },
