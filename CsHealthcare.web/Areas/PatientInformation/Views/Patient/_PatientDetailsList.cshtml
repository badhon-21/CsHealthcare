
@model IEnumerable<CsHealthcare.ViewModel.Patient.PatientDetailsModel>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TestName)
        </td>
        @*<td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.Discount)
        </td>*@
        <td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.Discount)
        </td>
        <td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.Total)
        </td>
        <td style="width: 17% !important">
            <a class="btn btn-xs btn-primary legitRipple" title="Edit" onclick="javascript: EditTest('@item.TestNameId');"><i class="icon-pencil"></i></a>
            <a class="btn btn-xs btn-danger" title="Delete" onclick="javascript: if (confirm('Are you sure you want to do operation?') == false) return false; DeleteTestDetails('@item.Id','@item.TestNameId'); "><i class="icon-trash"></i></a>
        </td>
    </tr>
}
