
@model IEnumerable<CsHealthcare.ViewModel.MedicineCorner.InPatientDrugSaleReturnDetailsModel>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DrugName)
        </td>
        @*<td style="width: 15% !important">
                @Html.DisplayFor(modelItem => item.Discount)
            </td>*@
        <td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.UnitPrice)
        </td>
        <td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.ReturnQty)
        </td>
       
        <td style="width: 15% !important">
            @Html.DisplayFor(modelItem => item.ReturnPrice)
        </td>
        <td style="width: 15% !important">
            <a class="btn btn-primary btn-icon legitRipple" title="Edit" onclick="javascript: EditTest('@item.DrugId');"><i class="icon-pencil"></i></a>
            <a class="btn btn-xs btn-danger" title="Delete" onclick="javascript: if (confirm('Are you sure you want to do operation?') == false) return false; DeleteTestDetails('@item.Id','@item.DrugId'); "><i class="icon-trash"></i></a>
        </td>
    </tr>
}

